      "commit-msg": "sf-husky-commit-msg",
      "pre-commit": "sf-husky-pre-commit",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "dependencies": {
    "@oclif/core": "^1.0.0",
    "@salesforce/core": "^3.6.5",
    "@salesforce/kit": "^1.5.17",
    "@salesforce/ts-types": "^1.5.20",
    "cli-ux": "^5.6.3",
    "inquirer": "^8.2.0"
  },
  "devDependencies": {
    "@salesforce/dev-config": "^2.0.0",
    "@salesforce/dev-scripts": "^0.9.18",
    "@salesforce/prettier-config": "^0.0.1",
    "@salesforce/ts-sinon": "^1.2.3",
    "@types/inquirer": "^8.1.3",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.0",
    "eslint-config-salesforce-license": "^0.1.0",
    "eslint-config-salesforce-typescript": "^0.1.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "shelljs": "0.8.1",
    "sinon": "^9.0.2",
    "strip-ansi": "6.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^4"
