    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "yarn sf-lint --fix",
    "postcompile": "tsc -p test",
    "prepack": "sf-prepack",
    "pretest": "sf-compile-test",
    "test": "sf-test"
  },
